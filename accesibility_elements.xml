			</combo>
			<text key="width" text="kerb width(e.g. 1.2 m)" it.text="Larghezza del cordolo(e.s. 1.2 m)"  />
			<text key="kerb:height" text="kerb height(e.g. 0.02 m)" it.text="Altezza del cordolo(e.s. 0.02 m)"  />
			<text key="incline" text="kerb inlination(e.g. 2.5%)" it.text="Inclinazione del cordolo(e.s. 2.5%)"  />
			<check key="tactile_paving" text="Presence of tactile paving on the kerb" it.text="Presenza di pavimento tattile" />
		</item>

		<!-- Schema for lowered kerb -->
		<item name="Lowered kerb" it.name="Cordolo ribassato" type="node" icon="https://github.com/LorenzoStucchi/accessibility_JOSM_preset/raw/master/symbols/kerb_orange.png">
